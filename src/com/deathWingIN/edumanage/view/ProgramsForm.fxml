<AnchorPane fx:id="context" prefHeight="729.0" prefWidth="804.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.deathWingIN.edumanage.controller.ProgramsFormController">
   <children>
      <Label layoutX="43.0" layoutY="55.0" text="Program Management" textFill="#0145ff">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <Separator layoutX="43.0" layoutY="91.0" prefHeight="0.0" prefWidth="716.0" />
      <Button layoutX="654.0" layoutY="57.0" mnemonicParsing="false" onAction="#backToHomeOnAction" style="-fx-background-color: #0C359E;" text="Back to Home" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="546.0" layoutY="57.0" mnemonicParsing="false" onAction="#newProgramOnAction" style="-fx-background-color: #3E3232;" text="+ New Program" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <TextField fx:id="txtId" editable="false" layoutX="43.0" layoutY="109.0" prefHeight="29.0" prefWidth="123.0" promptText="Program Code">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </TextField>
      <TextField fx:id="txtName" layoutX="189.0" layoutY="108.0" prefHeight="29.0" prefWidth="190.0" promptText="Program Name ">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </TextField>
      <TextField fx:id="txtSearch" layoutX="43.0" layoutY="447.0" prefHeight="29.0" prefWidth="521.0" promptText="Serach">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </TextField>
      <Button fx:id="btn" layoutX="591.0" layoutY="445.0" mnemonicParsing="false" onAction="#saveOnAction" prefHeight="29.0" prefWidth="165.0" style="-fx-background-color: #4F6F52;" text="Save Program" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <TableView fx:id="tblPrograms" layoutX="44.0" layoutY="493.0" prefHeight="212.0" prefWidth="716.0">
         <columns>
            <TableColumn fx:id="colId" prefWidth="75.0" text="#Code" />
            <TableColumn fx:id="colProgram" prefWidth="163.0" text="Program" />
            <TableColumn fx:id="colTeacher" prefWidth="130.0" text="Teacher" />
            <TableColumn fx:id="colTechnologies" prefWidth="139.0" text="Teachnologies" />
            <TableColumn fx:id="colCost" prefWidth="98.0" text="Cost" />
            <TableColumn fx:id="colOption" prefWidth="110.0" text="Option" />
         </columns>
      </TableView>
      <TextField fx:id="txtCost" layoutX="402.0" layoutY="109.0" prefHeight="29.0" prefWidth="129.0" promptText="Cost">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </TextField>
      <ComboBox fx:id="cmbTeacher" layoutX="550.0" layoutY="108.0" prefHeight="29.0" prefWidth="202.0" promptText="Teacher" />
      <TextField fx:id="txtTechnology" layoutX="43.0" layoutY="166.0" onAction="#addTechOnAction" prefHeight="29.0" prefWidth="294.0" promptText="Teachnology">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </TextField>
      <TableView fx:id="tblTechnologies" layoutX="44.0" layoutY="212.0" prefHeight="212.0" prefWidth="716.0">
         <columns>
            <TableColumn fx:id="colTCode" prefWidth="75.0" text="#ID" />
            <TableColumn fx:id="colTName" prefWidth="490.0" text="Teachnology" />
            <TableColumn fx:id="colTRemove" prefWidth="150.0" text="Remove" />
